{
  "presets": [
    "latest",
    "stage-2",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        },
        "useBuiltIns": true
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    [
      "transform-runtime",
      {
        "polyfill": true,
        "regenerator": false
      }
    ]
  ]
}
